#
# minecraft_world_api:worldborder/cache/set_static
#

# Default border size
data modify storage minecraft_world_api:static worldborder.DEFAULT_BORDER_SIZE set value 59999968.0d

# Default border center
data modify storage minecraft_world_api:static worldborder.DEFAULT_BORDER_CENTER_X set value 0.0d
data modify storage minecraft_world_api:static worldborder.DEFAULT_BORDER_CENTER_Z set value 0.0d

# Default border damage buffer
data modify storage minecraft_world_api:static worldborder.DEFAULT_BORDER_DAMAGE_BUFFER set value 5.0d

# Default border damage amount
data modify storage minecraft_world_api:static worldborder.DEFAULT_BORDER_DAMAGE_AMOUNT set value 0.2d
