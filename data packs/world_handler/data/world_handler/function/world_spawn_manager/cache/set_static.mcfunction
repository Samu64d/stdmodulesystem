#
# world_handler:world_spawn_manager/cache/set_static
#

# Default world spawn dimension
data modify storage world_handler:static world_spawn_manager.DEFAULT_WORLD_SPAWN_DIMENSION set value "minecraft:overworld"

# Default world spawn position
data modify storage world_handler:static world_spawn_manager.DEFAULT_WORLD_SPAWN_POSITION set value [0d, 0d, 0d]
