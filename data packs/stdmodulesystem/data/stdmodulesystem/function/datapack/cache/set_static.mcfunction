#
# stdmodulesystem:datapack/cache/set_static
#

# Vanilla datapack id
data modify storage stdmodulesystem:static datapack.VANILLA_DATAPACK_ID set value "vanilla"

# Vanilla datapack name
data modify storage stdmodulesystem:static datapack.VANILLA_DATAPACK_NAME set value "vanilla"

# Custom datapack suffix
data modify storage stdmodulesystem:static datapack.DATAPACK_SUFFIX set value "file/"
